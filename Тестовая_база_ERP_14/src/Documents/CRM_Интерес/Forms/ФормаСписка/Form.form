    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ДокументCRM_Интерес.Ссылка,
	ДокументCRM_Интерес.ПометкаУдаления,
	ДокументCRM_Интерес.Номер,
	ДокументCRM_Интерес.Дата,
	ДокументCRM_Интерес.Проведен,
	ДокументCRM_Интерес.Автор,
	ДокументCRM_Интерес.ВероятностьСделки,
	ДокументCRM_Интерес.ДокументОснование,
	ДокументCRM_Интерес.Завершен,
	ДокументCRM_Интерес.КонтактноеЛицо,
	ДокументCRM_Интерес.ОжидаемаяВыручка,
	ДокументCRM_Интерес.Описание,
	ДокументCRM_Интерес.Организация,
	ДокументCRM_Интерес.Ответственный,
	ДокументCRM_Интерес.Офис,
	ВЫБОР
		КОГДА ДокументCRM_Интерес.Партнер = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
			ТОГДА ДокументCRM_Интерес.ПотенциальныйКлиент
		ИНАЧЕ ДокументCRM_Интерес.Партнер
	КОНЕЦ КАК Партнер,
	ДокументCRM_Интерес.Подразделение,
	ДокументCRM_Интерес.ПотенциальныйКлиент,
	ДокументCRM_Интерес.ПричинаОтказа,
	ДокументCRM_Интерес.ТипУслуги,
	ДокументCRM_Интерес.СостояниеИнтереса,
	ДокументCRM_Интерес.СостояниеИнтереса.Родитель КАК Сценарий,
	ДокументCRM_Интерес.Тема,
	ДокументCRM_Интерес.ДатаЗакрытия,
	ДокументCRM_Интерес.МоментВремени,
	ВЫБОР
		КОГДА ДокументCRM_Интерес.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.УспешноеЗавершение)
			ТОГДА 1
		ИНАЧЕ ВЫБОР
				КОГДА ДокументCRM_Интерес.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.НеудачноеЗавершение)
					ТОГДА 2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК КартинкаСостояния,
	ВложенныйЗапрос.ПланируемаяДата КАК ДатаСледующейАктивности,
	ВложенныйЗапрос.Тема КАК СледующаяАктивность,
	ВЫБОР
		КОГДА ДокументCRM_Интерес.Партнер = ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
			ТОГДА 5
		КОГДА (ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиКИ + ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиПортрет) / 2 &lt; 25
			ТОГДА 0
		КОГДА (ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиКИ + ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиПортрет) / 2 &lt; 50
			ТОГДА 1
		КОГДА (ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиКИ + ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиПортрет) / 2 &lt; 75
			ТОГДА 2
		КОГДА (ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиКИ + ДокументCRM_Интерес.Партнер.CRM_ПроцентЗаполненностиПортрет) / 2 &lt; 90
			ТОГДА 3
		ИНАЧЕ 4
	КОНЕЦ КАК ЗаполненностьИнформации
ИЗ
	Документ.CRM_Интерес КАК ДокументCRM_Интерес
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			CRM_ЗапланированныеАктивности.Объект КАК Объект,
			МИНИМУМ(CRM_ЗапланированныеАктивности.ПланируемаяДата) КАК ПланируемаяДата,
			МИНИМУМ(CRM_ЗапланированныеАктивности.Тема) КАК Тема
		ИЗ
			РегистрСведений.CRM_ЗапланированныеАктивности КАК CRM_ЗапланированныеАктивности
				ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
					CRM_ЗапланированныеАктивности.Объект КАК Объект,
					МИНИМУМ(CRM_ЗапланированныеАктивности.ПланируемаяДата) КАК ПланируемаяДата
				ИЗ
					РегистрСведений.CRM_ЗапланированныеАктивности КАК CRM_ЗапланированныеАктивности
				
				СГРУППИРОВАТЬ ПО
					CRM_ЗапланированныеАктивности.Объект) КАК ВложенныйЗапрос
				ПО CRM_ЗапланированныеАктивности.Объект = ВложенныйЗапрос.Объект
					И CRM_ЗапланированныеАктивности.ПланируемаяДата = ВложенныйЗапрос.ПланируемаяДата
		
		СГРУППИРОВАТЬ ПО
			CRM_ЗапланированныеАктивности.Объект) КАК ВложенныйЗапрос
		ПО ДокументCRM_Интерес.Ссылка = ВложенныйЗапрос.Объект
