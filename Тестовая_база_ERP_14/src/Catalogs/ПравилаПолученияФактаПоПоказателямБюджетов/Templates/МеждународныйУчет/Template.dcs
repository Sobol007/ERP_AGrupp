	ИсточникДанных,
	Организация.*,
	Подразделение.*,
	НаправлениеДеятельности.*,
	Субконто1.*,
	Субконто2.*,
	Субконто3.*,
	Валюта.*,
	СуммаСальдо,
	СуммаСальдоДт,
	СуммаСальдоКт,
	СуммаВВалютеСальдо,
	СуммаВВалютеСальдоДт,
	СуммаВВалютеСальдоКт}
