			текОбъект = текСсылка.ПолучитьОбъект();
			текОбъект.НаименованиеПолное = Макет.Область(т, 2).Текст;
			текОбъект.Возврат = Не ПустаяСтрока(Макет.Область(т, 3).Текст);
			текОбъект.ПериодическиеФиксированныеПлатежи = Не ПустаяСтрока(Макет.Область(т, 4).Текст);
			текОбъект.Записать();
		КонецЕсли;
