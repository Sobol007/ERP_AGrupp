							<dcscor:value xsi:type="dcscor:Parameter">Интерес</dcscor:value>
						</dcscor:item>
					</dcsat:appearance>
				</dcsat:tableCell>
			</dcsat:item>
		</template>
		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>Дата</dcsat:name>
			<dcsat:expression>ДатаЗавершенияВзаимодействия</dcsat:expression>
		</parameter>
		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:DetailsAreaTemplateParameter">
			<dcsat:name>Интерес</dcsat:name>
			<dcsat:fieldExpression>
				<dcsat:field>ДокументОснование</dcsat:field>
				<dcsat:expression>ДокументОснование</dcsat:expression>
			</dcsat:fieldExpression>
			<dcsat:mainAction>OpenValue</dcsat:mainAction>
		</parameter>
	</template>
	<fieldTemplate>
		<field>Тема</field>
		<template>Макет1</template>
	</fieldTemplate>
	<fieldTemplate>
		<field>ДатаЗавершенияВзаимодействия</field>
