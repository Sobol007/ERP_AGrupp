		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	CRM_Интерес.Ссылка КАК Интерес,
	CRM_Интерес.ОжидаемаяВыручка КАК Сумма,
	CRM_Интерес.СостояниеИнтереса.Родитель КАК СостояниеИнтересаРодитель,
	CRM_Интерес.Описание КАК Описание,
	CRM_Интерес.ДатаЗакрытия КАК ДатаЗакрытия,
	CRM_Интерес.Автор КАК Автор,
	CRM_Интерес.Подразделение КАК Подразделение,
	CRM_Интерес.Организация КАК Организация,
	CRM_Интерес.Партнер КАК Партнер,
	CRM_Интерес.КонтактноеЛицо КАК КонтактноеЛицо,
	CRM_Интерес.Дата КАК Дата,
	1 КАК Количество,
	CRM_Интерес.Ответственный КАК Ответственный
ИЗ
	Документ.CRM_Интерес КАК CRM_Интерес
ГДЕ
	НЕ CRM_Интерес.ПометкаУдаления
	И НЕ CRM_Интерес.СостояниеИнтереса.ВидСостояния = ЗНАЧЕНИЕ(Перечисление.CRM_ВидыСостоянияИнтереса.УспешноеЗавершение)
	И CRM_Интерес.ДатаЗакрытия МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ВЫБОР
			КОГДА &amp;Менеджеры = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
				ТОГДА ИСТИНА
			ИНАЧЕ CRM_Интерес.Ответственный = &amp;Менеджеры
		КОНЕЦ</query>
	</dataSet>
