						ТОГДА &quot;  &quot; + ЗадачаЗадачаИсполнителя.CRM_Партнер.Наименование
					ИНАЧЕ &quot;&quot;
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ЗадачаЗадачаИсполнителя.БизнесПроцесс.Партнер &lt;> ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)
					ТОГДА ВЫБОР
							КОГДА ЗадачаЗадачаИсполнителя.CRM_ТочкаМаршрута &lt;> ЗНАЧЕНИЕ(Справочник.CRM_ТочкиМаршрутов.ПустаяСсылка)
									ИЛИ ЗадачаЗадачаИсполнителя.БизнесПроцесс.Проект &lt;> ЗНАЧЕНИЕ(Справочник.Проекты.ПустаяСсылка)
								ТОГДА &quot;  &quot;
							ИНАЧЕ &quot;&quot;
						КОНЕЦ + ЗадачаЗадачаИсполнителя.БизнесПроцесс.Партнер.Наименование
				ИНАЧЕ &quot;&quot;
			КОНЕЦ
	КОНЕЦ КАК Описание,
	ЗадачаЗадачаИсполнителя.Ссылка КАК Ссылка,
	ВЫБОР
		КОГДА ЗадачаЗадачаИсполнителя.Важность = ЗНАЧЕНИЕ(Перечисление.ВариантыВажностиЗадачи.Низкая)
			ТОГДА 0
		КОГДА ЗадачаЗадачаИсполнителя.Важность = ЗНАЧЕНИЕ(Перечисление.ВариантыВажностиЗадачи.Высокая)
			ТОГДА 2
		ИНАЧЕ 1
	КОНЕЦ КАК ВажностьКартинка,
	ЗадачаЗадачаИсполнителя.Важность КАК Важность,
	ВЫБОР
		КОГДА ЗадачаЗадачаИсполнителя.Выполнена
			ТОГДА 2
		КОГДА НЕ ЗадачаЗадачаИсполнителя.ПринятаКИсполнению
			ТОГДА 0
		КОГДА ЗадачаЗадачаИсполнителя.ПринятаКИсполнению
			ТОГДА 1
	КОНЕЦ КАК Состояние,
	ЗадачаЗадачаИсполнителя.ПринятаКИсполнению КАК ПринятаКИсполнению,
	ЗадачаЗадачаИсполнителя.Выполнена КАК Выполнена,
	ЗадачаЗадачаИсполнителя.ДатаНачала КАК ДатаНачала,
	ЗадачаЗадачаИсполнителя.СрокИсполнения КАК СрокИсполнения,
	ВЫБОР
		КОГДА ЗадачаЗадачаИсполнителя.Выполнена
			ТОГДА 1
		ИНАЧЕ ВЫБОР
				КОГДА ЗадачаЗадачаИсполнителя.СрокИсполнения = &amp;ПарамПустаяДата
					ТОГДА 1
				ИНАЧЕ ВЫБОР
						КОГДА ЗадачаЗадачаИсполнителя.СрокИсполнения &lt; &amp;ПарамТекущаяДата
							ТОГДА 0
						ИНАЧЕ 1
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ КАК Просрочена,
	ВЫБОР
		КОГДА ЗадачаЗадачаИсполнителя.Выполнена
			ТОГДА 0
		КОГДА ЗадачаЗадачаИсполнителя.СрокИсполнения = &amp;ПарамПустаяДата
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ЗадачаЗадачаИсполнителя.СрокИсполнения &lt; &amp;ПарамТекущаяДата
					ТОГДА РАЗНОСТЬДАТ(&amp;ПарамТекущаяДата, ЗадачаЗадачаИсполнителя.СрокИсполнения, ДЕНЬ) * -1
				ИНАЧЕ РАЗНОСТЬДАТ(&amp;ПарамТекущаяДата, ЗадачаЗадачаИсполнителя.СрокИсполнения, ДЕНЬ)
			КОНЕЦ
	КОНЕЦ КАК ОсталосьДней,
	ЗадачаЗадачаИсполнителя.Автор КАК Автор,
	ЗадачаЗадачаИсполнителя.Исполнитель КАК Исполнитель,
	ЗадачаЗадачаИсполнителя.РольИсполнителя КАК РольИсполнителя,
	ЗадачаЗадачаИсполнителя.ДатаИсполнения КАК ДатаИсполнения,
	ЗадачаЗадачаИсполнителя.ДатаПринятияКИсполнению КАК ДатаПринятияКИсполнению,
	ЗадачаЗадачаИсполнителя.CRM_Личная КАК Личная,
	ВЫБОР
		КОГДА ЗадачаЗадачаИсполнителя.РольИсполнителя = ЗНАЧЕНИЕ(Справочник.РолиИсполнителей.ПустаяСсылка)
			ТОГДА ЗадачаЗадачаИсполнителя.Исполнитель
		КОГДА НЕ ЗадачаЗадачаИсполнителя.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
			ТОГДА ЗадачаЗадачаИсполнителя.Исполнитель
		ИНАЧЕ ЗадачаЗадачаИсполнителя.РольИсполнителя
	КОНЕЦ КАК ИсполнительРоль,
	ЗадачаЗадачаИсполнителя.ПометкаУдаления КАК ПометкаУдаления,
	ЕСТЬNULL(ЗадачаЗадачаИсполнителя.CRM_ОсновнаяКатегория.ЦветИндекс, 26) КАК ОсновнаяКатегорияИндекс
ИЗ
	Задача.ЗадачаИсполнителя КАК ЗадачаЗадачаИсполнителя
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИсполнителиЗадач КАК ИсполнителиЗадач
		ПО ЗадачаЗадачаИсполнителя.РольИсполнителя = ИсполнителиЗадач.РольИсполнителя
			И (ИсполнителиЗадач.Исполнитель = &amp;Исполнитель
				ИЛИ ИсполнителиЗадач.Исполнитель.Подразделение В (&amp;Подразделения))
ГДЕ
	НЕ ЗадачаЗадачаИсполнителя.ПометкаУдаления
	И (&amp;Исполнитель ЕСТЬ NULL
			ИЛИ ЗадачаЗадачаИсполнителя.Исполнитель В (&amp;Исполнитель)
			ИЛИ ЗадачаЗадачаИсполнителя.Исполнитель.Подразделение В (&amp;Подразделения)
			ИЛИ ЗадачаЗадачаИсполнителя.Автор В (&amp;Исполнитель)
			ИЛИ ИСТИНА В
					(ВЫБРАТЬ
						ИСТИНА
					ИЗ
						РегистрСведений.ИсполнителиЗадач КАК ИсполнителиЗадач
					ГДЕ
						ЗадачаЗадачаИсполнителя.РольИсполнителя = ИсполнителиЗадач.РольИсполнителя
						И ЗадачаЗадачаИсполнителя.ОсновнойОбъектАдресации = ИсполнителиЗадач.ОсновнойОбъектАдресации
						И ЗадачаЗадачаИсполнителя.ДополнительныйОбъектАдресации = ИсполнителиЗадач.ДополнительныйОбъектАдресации
						И (ИсполнителиЗадач.Исполнитель = &amp;Исполнитель
							ИЛИ ИсполнителиЗадач.Исполнитель.Подразделение В (&amp;Подразделения)))
				И ЗадачаЗадачаИсполнителя.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка))
{ГДЕ
	(ВЫБОР
			КОГДА ЗадачаЗадачаИсполнителя.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
					ИЛИ ЗадачаЗадачаИсполнителя.Исполнитель = НЕОПРЕДЕЛЕНО
				ТОГДА ВЫБОР
						КОГДА ИсполнителиЗадач.Исполнитель = &amp;Исполнитель
							ТОГДА ИсполнителиЗадач.Исполнитель
						ИНАЧЕ ВЫБОР
								КОГДА ИсполнителиЗадач.Исполнитель ЕСТЬ NULL
									ТОГДА ЛОЖЬ
								ИНАЧЕ ИСТИНА
							КОНЕЦ
					КОНЕЦ
			ИНАЧЕ ВЫБОР
					КОГДА ЗадачаЗадачаИсполнителя.Исполнитель = &amp;Исполнитель
						ТОГДА ЗадачаЗадачаИсполнителя.Исполнитель
					ИНАЧЕ ВЫБОР
							КОГДА ЗадачаЗадачаИсполнителя.Исполнитель.Подразделение В (&amp;Подразделения)
								ТОГДА ИСТИНА
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
				КОНЕЦ
		КОНЕЦ) КАК ИсполнительПотенциальный}</queryText>
